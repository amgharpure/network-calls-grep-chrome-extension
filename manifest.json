{
    "manifest_version": 2,
    "name": "Network Calls Grep",
    "version": "1.0",
    "devtools_page": "devtools.html",
    "permissions": ["\u003Call_urls>"],
    "icons": {
        "48": "images/baseline_search_black_48dp.png"
    },
    "browser_action": {
        "default_popup": "popup.html",
